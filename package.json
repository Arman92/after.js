{
  "name": "@jaredpalmer/after",
  "private": false,
  "version": "1.5.1",
  "description": "Build isomorphic Javascript applications with ease.",
  "repository": "jaredpalmer/after.js",
  "author": "Jared Palmer <jared@palmer.net>",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/after.umd.production.js",
  "module": "dist/after.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format=cjs,esm,umd",
    "test": "tsdx test --env=jsdom",
    "test:watch": "npm run test -- --watchAll",
    "prepublishOnly": "npm run build",
    "lint": "tsdx lint"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "peerDependencies": {
    "express": ">=4",
    "react": ">=16.3.2",
    "react-helmet": ">=5.2.0",
    "react-router-dom": ">=4.2.2",
    "serialize-javascript": ">=1.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^10.9.2",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/serialize-javascript": "^1.3.2",
    "express": "^4.16.2",
    "husky": "^3.1.0",
    "razzle": "^2.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^2.1.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.3"
  }
}
